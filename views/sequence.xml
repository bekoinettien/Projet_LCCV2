<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--data noupdate="1"-->
	<data>


        <record id="seq_farmer_slip" model="ir.sequence">
            <field name="name">Farmer Salary Slip</field>
            <field name="code">farmer.slip</field>
            <field name="prefix">PL/%(woy)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_farmer" model="ir.sequence">
            <field name="name">Code Planteur</field>
            <field name="code">farmer.code</field>
            <field name="padding">6</field>
        </record>

        <odoo>
    <data noupdate="1">
        <record id="seq_paiement_reference" model="ir.sequence">
            <field name="name">Paiement Reference</field>
            <field name="code">paiement.reference</field>
            <field name="prefix">PAY_PRIME_EXCEPTIONNELLE/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>


    </data>
</odoo>
